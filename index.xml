    <item>
      <title>Clickable Image Slider</title>
      <link>/slider1/</link>
      <pubDate>Wed, 02 Oct 2024 00:00:00 +0000</pubDate>
      <guid>/slider1/</guid>
      <description>&lt;script src=&#34;/rmarkdown-libs/htmlwidgets/htmlwidgets.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/jquery/jquery-3.4.1.min.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/jquery-migrate/jquery-migrate-3.1.0.min.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/slick/slick/slick.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/slick/slick/slick-theme.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/slick/slick/slick.min.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/css-resize/ResizeSensor.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/css-resize/ElementQueries.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/slickR/slick.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/slickR/slick-theme.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/slickR/slickR.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/htmltools-fill/fill.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/htmlwidgets/htmlwidgets.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/jquery/jquery-3.6.0.min.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/leaflet/leaflet.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/leaflet/leaflet.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/leafletfix/leafletfix.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/proj4/proj4.min.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/Proj4Leaflet/proj4leaflet.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;link href=&#34;/rmarkdown-libs/rstudio_leaflet/rstudio_leaflet.css&#34; rel=&#34;stylesheet&#34; /&gt;&#xD;&#xA;&lt;script src=&#34;/rmarkdown-libs/leaflet-binding/leaflet.js&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;p&gt;Coba Clickable Image Slider&lt;/p&gt;&#xA;&lt;div class=&#34;slider-container&#34;&gt;&#xD;&#xA;&lt;div class=&#34;slider&#34;&gt;&#xD;&#xA;&lt;pre&gt;&lt;code&gt;&amp;lt;img src=&amp;quot;/./tabel_berau.png&amp;quot; class=&amp;quot;slider-image&amp;quot; alt=&amp;quot;Image 1&amp;quot; /&amp;gt;&#xD;&#xA;&amp;lt;img src=&amp;quot;/./feedback.png&amp;quot; class=&amp;quot;slider-image&amp;quot; alt=&amp;quot;Image 2&amp;quot; /&amp;gt;&#xD;&#xA;&amp;lt;img src=&amp;quot;/./berau_lead.png&amp;quot; class=&amp;quot;slider-image&amp;quot; alt=&amp;quot;Image 3&amp;quot; /&amp;gt;&#xD;&#xA;&lt;/code&gt;&lt;/pre&gt;&#xA;&lt;/div&gt;&#xD;&#xA;&lt;p&gt;&lt;a class=&#34;prev&#34; onclick=&#34;plusSlides(-1)&#34;&gt;❮&lt;/a&gt;&#xA;&lt;a class=&#34;next&#34; onclick=&#34;plusSlides(1)&#34;&gt;❯&lt;/a&gt;&lt;/p&gt;&#xA;&lt;/div&gt;&#xD;&#xA;&lt;style&gt;&#xD;&#xA;/* Slider container */&#xD;&#xA;.slider-container {&#xD;&#xA;  position: relative;&#xD;&#xA;  max-width: 600px;&#xD;&#xA;  margin: auto;&#xD;&#xA;  overflow: hidden;&#xD;&#xA;}&#xD;&#xA;&amp;#10;/* Hide all images by default */&#xD;&#xA;.slider-image {&#xD;&#xA;  display: none;&#xD;&#xA;  width: 100%;&#xD;&#xA;}&#xD;&#xA;&amp;#10;/* Style the navigation arrows */&#xD;&#xA;.prev, .next {&#xD;&#xA;  cursor: pointer;&#xD;&#xA;  position: absolute;&#xD;&#xA;  top: 50%;&#xD;&#xA;  width: auto;&#xD;&#xA;  padding: 16px;&#xD;&#xA;  margin-top: -22px;&#xD;&#xA;  color: white;&#xD;&#xA;  font-weight: bold;&#xD;&#xA;  font-size: 18px;&#xD;&#xA;  transition: 0.6s ease;&#xD;&#xA;  border-radius: 0 3px 3px 0;&#xD;&#xA;  user-select: none;&#xD;&#xA;}&#xD;&#xA;&amp;#10;.next {&#xD;&#xA;  right: 0;&#xD;&#xA;  border-radius: 3px 0 0 3px;&#xD;&#xA;}&#xD;&#xA;&amp;#10;.prev:hover, .next:hover {&#xD;&#xA;  background-color: rgba(0, 0, 0, 0.8);&#xD;&#xA;}&#xD;&#xA;&lt;/style&gt;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;let slideIndex = 1;&#xD;&#xA;showSlides(slideIndex);&#xD;&#xA;&amp;#10;// Function to show slides&#xD;&#xA;function showSlides(n) {&#xD;&#xA;  let i;&#xD;&#xA;  let slides = document.getElementsByClassName(&#34;slider-image&#34;);&#xD;&#xA;  if (n &gt; slides.length) {slideIndex = 1}&#xD;&#xA;  if (n &lt; 1) {slideIndex = slides.length}&#xD;&#xA;  for (i = 0; i &lt; slides.length; i++) {&#xD;&#xA;    slides[i].style.display = &#34;none&#34;;  &#xD;&#xA;  }&#xD;&#xA;  slides[slideIndex - 1].style.display = &#34;block&#34;;  &#xD;&#xA;}&#xD;&#xA;&amp;#10;// Function to move to next or previous slide&#xD;&#xA;function plusSlides(n) {&#xD;&#xA;  showSlides(slideIndex += n);&#xD;&#xA;}&#xD;&#xA;&lt;/script&gt;&#xD;&#xA;&lt;div id=&#34;htmlwidget-1&#34; style=&#34;width:95%;height:480px;&#34; class=&#34;slickR html-widget&#34;&gt;&lt;/div&gt;&#xD;&#xA;&lt;script type=&#34;application/json&#34; data-for=&#34;htmlwidget-1&#34;&gt;{&#34;x&#34;:[{&#34;obj&#34;:&#34;&lt;div class=\&#34;mySlider_bump_442289863713086\&#34;&gt;\n  &lt;div&gt;\n    &lt;img src=\&#34;https://via.placeholder.com/600x400.png?text=Image+1\&#34; style=\&#34;width:95%;height:99%;margin-left:auto;margin-right:auto;\&#34;/&gt;\n  &lt;\/div&gt;\n  &lt;div&gt;\n    &lt;img src=\&#34;https://via.placeholder.com/600x400.png?text=Image+2\&#34; style=\&#34;width:95%;height:99%;margin-left:auto;margin-right:auto;\&#34;/&gt;\n  &lt;\/div&gt;\n  &lt;div&gt;\n    &lt;img src=\&#34;https://via.placeholder.com/600x400.png?text=Image+3\&#34; style=\&#34;width:95%;height:99%;margin-left:auto;margin-right:auto;\&#34;/&gt;\n  &lt;\/div&gt;\n&lt;\/div&gt;&#34;,&#34;divName&#34;:&#34;mySlider_bump_442289863713086&#34;,&#34;slideh&#34;:&#34;99%&#34;,&#34;slidew&#34;:&#34;95%&#34;,&#34;slickOpts&#34;:{&#34;autoplay&#34;:true,&#34;autoplaySpeed&#34;:1000,&#34;dots&#34;:true}}],&#34;evals&#34;:[],&#34;jsHooks&#34;:[]}&lt;/script&gt;&#xD;&#xA;&lt;div class=&#34;leaflet html-widget html-fill-item&#34; id=&#34;htmlwidget-2&#34; style=&#34;width:672px;height:480px;&#34;&gt;&lt;/div&gt;&#xD;&#xA;&lt;script type=&#34;application/json&#34; data-for=&#34;htmlwidget-2&#34;&gt;{&#34;x&#34;:{&#34;options&#34;:{&#34;crs&#34;:{&#34;crsClass&#34;:&#34;L.CRS.EPSG3857&#34;,&#34;code&#34;:null,&#34;proj4def&#34;:null,&#34;projectedBounds&#34;:null,&#34;options&#34;:{}}},&#34;calls&#34;:[{&#34;method&#34;:&#34;addTiles&#34;,&#34;args&#34;:[&#34;https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png&#34;,null,null,{&#34;minZoom&#34;:0,&#34;maxZoom&#34;:18,&#34;tileSize&#34;:256,&#34;subdomains&#34;:&#34;abc&#34;,&#34;errorTileUrl&#34;:&#34;&#34;,&#34;tms&#34;:false,&#34;noWrap&#34;:false,&#34;zoomOffset&#34;:0,&#34;zoomReverse&#34;:false,&#34;opacity&#34;:1,&#34;zIndex&#34;:1,&#34;detectRetina&#34;:false,&#34;attribution&#34;:&#34;&amp;copy; &lt;a href=\&#34;https://openstreetmap.org/copyright/\&#34;&gt;OpenStreetMap&lt;\/a&gt;,  &lt;a href=\&#34;https://opendatacommons.org/licenses/odbl/\&#34;&gt;ODbL&lt;\/a&gt;&#34;}]},{&#34;method&#34;:&#34;addMarkers&#34;,&#34;args&#34;:[40.4,-3.7,null,null,null,{&#34;interactive&#34;:true,&#34;draggable&#34;:false,&#34;keyboard&#34;:true,&#34;title&#34;:&#34;&#34;,&#34;alt&#34;:&#34;&#34;,&#34;zIndexOffset&#34;:0,&#34;opacity&#34;:1,&#34;riseOnHover&#34;:false,&#34;riseOffset&#34;:250},null,null,null,null,null,{&#34;interactive&#34;:false,&#34;permanent&#34;:false,&#34;direction&#34;:&#34;auto&#34;,&#34;opacity&#34;:1,&#34;offset&#34;:[0,0],&#34;textsize&#34;:&#34;10px&#34;,&#34;textOnly&#34;:false,&#34;className&#34;:&#34;&#34;,&#34;sticky&#34;:true},null]}],&#34;setView&#34;:[[40.4,-3.7],5,[]],&#34;limits&#34;:{&#34;lat&#34;:[40.4,40.4],&#34;lng&#34;:[-3.7,-3.7]}},&#34;evals&#34;:[],&#34;jsHooks&#34;:[]}&lt;/script&gt;</description>
    </item>
